<% array(page.embed).forEach(data=> { %>
<section id="<%- data.type %>">
  <div class="container">
    <div class="mb-3">
      <h2><%- __(`post.embed.${data.type}.title`) %></h2>
      <span><%- __(`post.embed.${data.type}.content`) %></span>
    </div>
    <% array(data.links).forEach(link=> { %>
    <div class="mb-3">
      <div class="position-relative border" style="padding-bottom: 56.25%">
        <iframe class="position-absolute w-100 h-100" src="<%- embed_link(link) %>"></iframe>
      </div>
    </div>
    <% }) %>
  </div>
</section>
<% }) %>