<%if(page.pictures != null ){ %>
    <section id="picture">
      <div class="container">
        <%if(page.pictures.length==1){ %>
        <div class="row">
          <div class="col-12">
            <img src="<%- page.pictures[0] %>" class="img-169" />
          </div>
        </div>
        <% }else if(page.pictures.length==2){ %>
        <div class="row">
          <div class="col-12">
            <img src="<%- page.pictures[0] %>" class="img-169" />
          </div>
          <div class="col-12">
            <img src="<%- page.pictures[1] %>" class="img-169" />
          </div>
        </div>
        <% }else if(page.pictures.length==3){ %>
        <div class="row">
          <div class="col-md-8">
            <img src="<%- page.pictures[0] %>" class="img-169" />
          </div>
          <div class="col-md-4 d-flex flex-column justify-content-between">
            <img src="<%- page.pictures[1] %>" class="img-169" />
            <img src="<%- page.pictures[2] %>" class="img-169" />
          </div>
        </div>
        <% }else if(page.pictures.length==4){ %>
        <div class="row">
          <div class="col-12">
            <img src="<%- page.pictures[0] %>" class="img-169" />
          </div>
          <div class="col-md-8">
            <img src="<%- page.pictures[1] %>" class="img-169" />
          </div>
          <div class="col-md-4 d-flex flex-column justify-content-between">
            <img src="<%- page.pictures[2] %>" class="img-169" />
            <img src="<%- page.pictures[3] %>" class="img-169" />
          </div>
        </div>
        <% }else if(page.pictures.length==5){ %>
        <div class="row">
          <div class="col-12">
            <img src="<%- page.pictures[0] %>" class="img-169" />
          </div>
          <div class="col-md-8">
            <img src="<%- page.pictures[1] %>" class="img-169" />
          </div>
          <div class="col-md-4 d-flex flex-column justify-content-between">
            <img src="<%- page.pictures[2] %>" class="img-169" />
            <img src="<%- page.pictures[3] %>" class="img-169" />
          </div>
          <div class="col-12">
            <img src="<%- page.pictures[3] %>" class="img-169" />
          </div>
        </div>
        <% } %>
      </div>
    </section>
<% } %>
    